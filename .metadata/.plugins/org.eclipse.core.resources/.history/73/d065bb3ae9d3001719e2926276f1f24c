package day01;

/**
 * @author Administrator
 *
 */
public class ChatAt {
	public static void main(String[] args) {
		String str = "123454321";
		for(int i=0; i<str.length()/2; i++){
			if(str.charAt(i)!=str.charAt(str.length()-1-i)){
				System.out.println(str+"是回文数！");
				break;
			}else{
				 System.out.println(str+"不是回文数！");
			}
			   
		}
	}
}
