package day04;
/**
 * @author Joshua
 *
 */
public class PointTest {

	public static void main(String[] args) {
	  Point<Integer> p1 = new Point<Integer>(1,2);
	  p1.setX(2);
	  double x1 = p1.getX();
	  System.out.println(x1);
	  
	  Point<Double> p2 = new Point<Double>(1.1,2.2);
	  p2.setX(2.2);
	  double x2 = p2.getX();
	  System.out.println(x2);
	}

}
